extends ../layouts/default-layout

block vars
	-title = 'Error 404 not found'
	-bodyClass = 'not-found-page'
	-des=''

block content 
	section(class="section-not-found")
		.container
			.row 
				.col-lg-7.d-flex.justify-content-center.align-items-center
					.not-found__content
						h2 404 Error 
						p Chúng tôi không tìm thấy kết quả nào phù hợp với tìm kiếm của bạn 
						p Xin vui trở về trang chủ
						.not-fount__btn
							+btn("Trang chủ","index.html")
				.col-lg-5.not-found__img.d-flex.justify-content-center.align-items-center
					img(src="../images/not-fount.png" class="img-fluid" alt="thanh-cong")
